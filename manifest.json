    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": ["tabs", "storage"] //need to add tab permissions so i can have access to tab urls, 
    //favIconUrl, and title etc..
    // ["storage"] /to use chrome's localStorage API
}
